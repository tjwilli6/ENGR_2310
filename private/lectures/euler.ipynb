   "source": [
    "def deriv(N,t,tau):\n",
    "    return -N / tau\n",
    "\n",
    "tau = 100\n",
    "t0 = 0\n",
    "tf = 10\n",
    "dt = 1\n",
    "\n",
    "N = 5\n",
    "for t in np.arange(t0,tf+dt,dt):\n",
    "    Nprime = deriv(N,t,tau)\n",
    "    N = N + Nprime * dt\n",
    "print(N)"
   ]
